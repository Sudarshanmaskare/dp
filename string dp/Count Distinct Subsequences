int distinctSubsequences(string s)
	{
	    // Your code goes here
	    int n=s.size();
	     vector<long long> last(256,-1);
	    vector<int> dp(n+1,0);
	    dp[0]=1;
	    for(int i=1;i<=n;i++){
	        dp[i]=(2*dp[i-1])%1000000007;
	        
	        if(last[s[i-1]]!=-1){
	            dp[i]=(dp[i]-dp[last[s[i-1]]])%1000000007;
	        }
	        last[s[i-1]]=i-1;
	    }
	    
	    if(dp[n]<0) dp[n]+=1000000007;
	    return dp[n];
	}
