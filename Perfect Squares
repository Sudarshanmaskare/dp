class Solution {
public:
    vector<int> dp;
    int recur(int n){
        if(n==1) return 1;
        if(n<1) return 0;
        if(dp[n]!=-1) return dp[n];
        int res=INT_MAX;
        for(int j=1;j<=sqrt(n);j++){
            res=min(1+recur(n-j*j),res);
        }
        return dp[n]=res;
    }
    int numSquares(int n) {
        dp.resize(n+1,-1);
       return recur(n);
    }
};
